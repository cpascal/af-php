{
  "development": {
    "locale":"en",
    "timezone":"Asia/Seoul",
    "log_enables":["emergency", "alert", "critical", "error", "warning", "notice", "info", "debug"],
    "display_errors": true,
    "log_handler": {
      "type":"file", 
      "path":"logs"
    },
    "timezone":"Asia/Seoul"
  },
  "production": {
    "locale":"en",
    "timezone":"Asia/Seoul",
    "log_enables":["emergency", "alert", "critical", "error"],
    "display_errors": false,
    "log_handler": {
      "type":"file", 
      "path":"logs"
    },
    "timezone":"Asia/Seoul"
  }
}
